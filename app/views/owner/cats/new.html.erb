<div class="container col-xs-12 col-md-4 col-md-offset-4">
  <h2>Add a cat</h2>
  <%= simple_form_for([:owner, @cat]) do |f| %>
  <%= f.input :name %>
  <%= f.input :breed, collection: @breed %>
  <%# f.collection_select :breed, Cat.order(:breed),:id,:breed, include_blank: true %>
  <%= f.input :description %>
  <%= f.input :birth_date, as: :string, required: false, input_html: {class: "datepicker"} %>
  <%= f.input :address %>
  <%= f.input :price_per_day %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit, class: 'btn btn-primary' %>
  <% end %>
</div>
